{
	"name": "@codeeditorland/tauricon",
	"description": "Make icons for your Tauri app.",
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/tauri"
	},
	"contributors": [
		"Tauri Programme within The Commons Conservancy"
	],
	"bin": {
		"tauricon": "./bin/tauricon.js"
	},
	"files": [
		"bin",
		"dist",
		"scripts",
		"src/helpers/icns.json"
	],
	"scripts": {
		"build": "rimraf ./dist && rollup -c --silent",
		"build-release": "rimraf ./dist && cross-env NODE_ENV=production rollup -c",
		"lint:lockfile": "lockfile-lint --path yarn.lock --type yarn --validate-https --allowed-hosts npm yarn",
		"prepublishOnly": "yarn build-release"
	},
	"resolutions": {
		"**/semver-regex": "4.0.3",
		"**/trim-newlines": "4.0.2"
	},
	"dependencies": {
		"@fiahfy/icns": "0.0.7",
		"chalk": "5.3.0",
		"cross-env": "7.0.3",
		"cross-spawn": "7.0.6",
		"find-up": "7.0.0",
		"fs-extra": "11.2.0",
		"glob": "11.0.0",
		"global-agent": "3.0.0",
		"got": "14.4.5",
		"imagemin": "9.0.0",
		"imagemin-optipng": "8.0.0",
		"imagemin-zopfli": "7.0.0",
		"inquirer": "12.1.0",
		"is-png": "3.0.1",
		"minimist": "1.2.8",
		"ms": "2.1.3",
		"png-to-ico": "2.1.8",
		"read-chunk": "5.0.0",
		"semver": "7.6.3",
		"sharp": "0.33.5",
		"update-notifier": "7.3.1"
	},
	"devDependencies": {
		"@types/cross-spawn": "6.0.6",
		"@types/fs-extra": "11.0.4",
		"@types/glob": "8.1.0",
		"@types/global-agent": "2.1.3",
		"@types/imagemin": "9.0.0",
		"@types/imagemin-optipng": "5.2.4",
		"@types/inquirer": "9.0.7",
		"@types/ms": "0.7.34",
		"@types/semver": "7.5.8",
		"@types/sharp": "0.32.0",
		"covector": "0.12.3",
		"is-running": "2.1.0",
		"lockfile-lint": "4.14.0",
		"promise": "8.3.0",
		"rimraf": "6.0.1",
		"tslib": "2.8.1"
	}
}
